{
  "presets": [
    "react",
    ["env",  { "modules": false }],
    "stage-0"
  ],
  "plugins": [
    "react-hot-loader/babel",
    "transform-object-assign",
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "transform-decorators-legacy",
    ["import", { libraryName: "antd", style: "css" }]],
  "comments": false
}
